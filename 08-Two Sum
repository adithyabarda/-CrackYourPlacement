class Solution {
    public int[] twoSum(int[] nums, int target) {
       HashMap<Integer,Integer> ans = new HashMap<>();
       int arr[]=new int[2];
       for(int i=0;i<nums.length;i++)
       {
        if(ans.containsKey(target-nums[i]))
        {
           arr[0]=i;
           arr[1]=ans.get(target-nums[i]);
        }
        else{
            ans.put(nums[i],i);
        }
       }
    return arr;
    }
}
