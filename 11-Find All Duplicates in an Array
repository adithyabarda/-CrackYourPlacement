class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        List<Integer> ans  = new ArrayList<>();
        for(int i=0;i<nums.length;i++)
        {
           int r=Math.abs(nums[i])-1;
           if(nums[r]<0)
           {
            ans.add(Math.abs(r+1));
           }
           else{
             nums[r]*=-1;
           }
        }
        return ans;
    }
}
